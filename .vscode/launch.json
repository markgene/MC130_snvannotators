    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "snvannotators"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "tests all",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest"
        },
        {
            "name": "tests.myvariant",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": [
                "discover",
                "-v",
                "tests.myvariant"
            ],
        },
        {
            "name": "tests.myvariant.annotators.test_myvarianthgvschrbatchannotator",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": [
                "tests.myvariant.annotators.test_myvarianthgvschrbatchannotator"
            ],
        },
        {
            "name": "tests.oncokb",
            "type": "debugpy",
            "request": "launch",
            "module": "unittest",
            "args": [
                "discover",
                "-v",
                "tests.oncokb"
            ],
        }
    ]
